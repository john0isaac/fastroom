{
  "name": "fast-room-web",
  "description": "Frontend for FastRoom using Vue 3 and Pinia",
  "private": true,
  "version": "0.1.0",
  "author": "John Aziz <johnaziz269@gmail.com>",
  "type": "module",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=8.0.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 5173",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .js,.ts,.vue",
    "lint:fix": "eslint . --ext .js,.ts,.vue --fix",
    "test": "echo \"No tests specified\" && exit 0",
    "generate:client": "openapi-ts --input http://127.0.0.1:8000/openapi.json --output src/generated/openapiclient --client @hey-api/client-axios"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@hey-api/client-axios": "^0.9.1",
    "@hey-api/openapi-ts": "0.80.5",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^24.2.1",
    "@types/vue": "^2.0.0",
    "@types/vue-router": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.4.0",
    "typescript-eslint": "^8.39.0",
    "vite": "^5.2.0",
    "vue-eslint-parser": "^10.2.0"
  }
}
